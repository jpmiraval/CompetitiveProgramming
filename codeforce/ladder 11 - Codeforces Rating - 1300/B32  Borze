#include <iostream>
#include <bits/stdc++.h>

using namespace std;

int main() {
  string cadena;

  cin >> cadena;

  for(int i = 0; i<cadena.size(); i++){
    if(cadena[i]==46){
      cout << 0;
    }else if(cadena[i]==45){
      if(cadena[i+1]==46){
        cout << 1;
      }else{
        cout << 2;
      }
    i++;}
  }
}
