#include <iostream>
#include <bits/stdc++.h>

using namespace std;

int main() {
  int matriz[5][5];
  int posicioni;
  int posicionj;
  int contador = 0;

  for(int i = 0; i<5; i++){
    for(int k = 0; k<5; k++){
      cin >> matriz[i][k];
      if(matriz[i][k]==1){
        posicioni = i;
        posicionj = k;
      }
    }
  }

cout << (abs(posicioni-2) + abs(posicionj-2));

}
